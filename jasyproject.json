{
	"name" : "core",
	"package" : "",
	
	"requires" : 
	[
		"../qunit",
		"../XRegExp",
		
		{
			"source" : "../history.js",
			"config" :
			{
				"files" :
				{
					"scripts/uncompressed/history.adapter.jquery.js" : "History.jQuery",
					"scripts/uncompressed/history.adapter.mootools.js" : "History.Mootools",
					"scripts/uncompressed/history.adapter.native.js" : "History.Native",
					"scripts/uncompressed/history.adapter.right.js" : "History.Right",
					"scripts/uncompressed/history.adapter.zepto.js" : "History.Zepto",
					"scripts/uncompressed/history.html4.js" : "History.HTML4",
					"scripts/uncompressed/history.js" : "History",
					"scripts/uncompressed/json2.js" : "JSON"
				}
			}
		},
		
		"../underscore"
	],
	
	"fields" :
	{
		"debug" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.Param"
		},

		"es5" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.ES5"
		},

		"engine" : 
		{
			"check" : ["webkit", "gecko", "trident", "presto"],
			"detect" : "core.detect.Engine"
		},

		"locale" : 
		{
			"check" : "String",
			"default" : "en",
			"detect" : "core.detect.Locale"
		}
	}
}
