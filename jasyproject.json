{
	"name" : "core",
	"package" : "",
	
	"requires" : 
	[
		{
			"source" : "../qunit",
			"config" :
			{
				"content" :
				{
					"qunit" : [
						"qunit/qunit.js"
					],

					"qunit.css" : [
						"qunit/qunit.css"
					]
				}
			}
		},
		
		{
			"source" : "../XRegExp",
			"config" :
			{
				"name" : "xregexp",
				"content" :
				{
					"XRegExp" : [
						"src/xregexp.js"
					]
				}
			}
		},
		
		{
			"source" : "../history.js",
			"config" :
			{
				"name" : "history",
				"content" :
				{
					"History" : 
					[
						"scripts/uncompressed/history.js",
						"scripts/uncompressed/history.html4.js",
						"scripts/uncompressed/history.adapter.jquery.js"
					]
				}
			}
		},

		{
			"source" : "../underscore",
			"config" :
			{
				"name" : "underscore",
				"content" :
				{
					"_" : [
						"underscore.js"
					]
				}
			}
		}
	],
	
	"fields" :
	{
		"debug" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.Param"
		},

		"es5" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.ES5"
		},

		"engine" : 
		{
			"check" : ["webkit", "gecko", "trident", "presto"],
			"detect" : "core.detect.Engine"
		},

		"locale" : 
		{
			"check" : "String",
			"default" : "en",
			"detect" : "core.detect.Locale"
		}
	}
}
