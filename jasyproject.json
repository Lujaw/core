{
	"name" : "core",
	"package" : "",

	"requires": [
		"git@github.com:zynga/apibrowser.git"
	],

	"fields" :
	{
		"debug" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.Param"
		},

		"es5" : 
		{
			"check" : "Boolean",
			"default" : false,
			"detect" : "core.detect.ES5"
		},

		"engine" : 
		{
			"check" : ["webkit", "gecko", "trident", "presto"],
			"detect" : "core.detect.Engine"
		},

		"locale" : 
		{
			"check" : "String",
			"default" : "en",
			"detect" : "core.detect.Locale"
		}
	}
}
